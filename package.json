{
  "name": "frontend",
  "version": "0.0.42",
  "templateVersion": "0.0.1",
  "author": "Nasr Aldin <ns@nasraldin.com> (https://nasraldin.com)",
  "description": "frontend app build with react js",
  "keywords": [
    "buy",
    "rent",
    "search"
  ],
  "repository": "https://gitlab.com/AJRLY/client/frontend.git",
  "license": "UNLICENSED",
  "private": true,
  "packageManager": "pnpm@7.16.1",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "source": "public/index.html",
  "scripts": {
    "dev": "parcel --port=3000 --lazy",
    "build": "parcel build",
    "build:serve": "npx serve build",
    "analyzer": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "analyzer:buddy": "parcel build --reporter @parcel/reporter-bundle-buddy",
    "check": "tsc --noEmit",
    "webhint": "npx hint http://localhost:3000",
    "lint": "pnpm eslint:check && pnpm stylelint:check && pnpm format:check",
    "lint:fix": "pnpm eslint && pnpm scss:unused && pnpm stylelint && pnpm format",
    "graph:dependencies": "madge --extensions=ts src -i graph-dps/dependencies.svg",
    "graph:dependencies:circular": "madge --circular --extensions=ts src -i graph-dps/circular.svg",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "eslint": "eslint src/ --ext ts,tsx --quiet --fix",
    "eslint:check": "eslint src/ --ext ts,tsx",
    "eslint:docs": "eslint -o ./linters/eslint.html -f html",
    "stylelint": "stylelint --quiet --fix src/*.scss",
    "stylelint:check": "stylelint --quiet src/*.scss",
    "scss:unused": "fusv src/",
    "pretty-quick": "pretty-quick",
    "clean:out": "rimraf build dist linters coverage .parcel-cache .history hint-report graph-dps",
    "clean:lock": "rimraf yarn.lock package-lock.json yarn-error.log",
    "clean:node": "rimraf node_modules pnpm-lock.yaml",
    "xClean": "pnpm clean:out && pnpm clean:lock && pnpm clean:node",
    "refresh": "pnpm clean:out && pnpm clean:lock && rimraf node_modules && pnpm install",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "security:audit": "pnpm audit",
    "security:audit:fix": "pnpm audit --fix",
    "packages:upgrade": "pnpm up --latest"
  },
  "targets": {
    "default": {
      "distDir": "./build"
    }
  },
  "alias": {
    "process": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "4.18.0",
    "ajv": "8.11.2",
    "axios": "1.2.0",
    "bootstrap": "5.2.3",
    "class-validator": "0.13.2",
    "clsx": "1.2.1",
    "flag-icons": "6.6.6",
    "framer-motion": "7.6.15",
    "idb": "7.1.1",
    "localstorage-slim": "2.3.0",
    "lodash": "4.17.21",
    "photoviewer": "3.6.5",
    "react": "^18.2.0",
    "react-bootstrap": "2.6.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.40.0",
    "react-hot-toast": "2.4.0",
    "react-select": "5.6.1",
    "redis": "4.5.1",
    "screenfull": "6.0.2",
    "ua-parser-js": "1.0.32",
    "uuid": "9.0.0",
    "web-vitals": "^3.1.0",
    "xregexp": "5.1.1",
    "zustand": "4.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@hookform/devtools": "4.2.3",
    "@parcel/compressor-brotli": "^2.8.0",
    "@parcel/compressor-gzip": "^2.8.0",
    "@parcel/config-default": "^2.8.0",
    "@parcel/core": "^2.8.0",
    "@parcel/packager-raw-url": "^2.8.0",
    "@parcel/reporter-cli": "^2.8.0",
    "@parcel/resolver-default": "^2.8.0",
    "@parcel/transformer-sass": "^2.8.0",
    "@parcel/transformer-svg-react": "^2.8.0",
    "@parcel/transformer-webmanifest": "^2.8.0",
    "@parcel/validator-typescript": "^2.8.0",
    "@tanstack/react-query-devtools": "4.18.0",
    "@types/lodash": "4.14.190",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "cross-env": "7.0.3",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.5.0",
    "find-unused-sass-variables": "4.0.5",
    "husky": "8.0.2",
    "lint-staged": "13.0.4",
    "parcel": "^2.8.0",
    "prettier": "2.8.0",
    "pretty-quick": "3.1.3",
    "rimraf": "3.0.2",
    "stylelint": "14.15.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended-scss": "8.0.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.3.0",
    "typescript": "^4.9.3"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "0.1.8"
    }
  }
}
